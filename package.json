{
  "name": "bensmith.sh",
  "type": "module",
  "version": "0.0.1",
  "description": "Ben Smith's website",
  "scripts": {
    "debug:dev": "DEBUG=Eleventy* npm run dev",
    "debug:prod": "DEBUG=Eleventy* npm run build",
    "dev": "eleventy --incremental --watch --serve",
    "build": "rm -rf _site && eleventy",
    "preview": "npm run build && http-server -p 2323 _site",
    "postinstall": "patch-package"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.5",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@sindresorhus/slugify": "^2.2.1",
    "eleventy-plugin-validate": "^0.1.2",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "markdown-it-anchor": "^8.6.7",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "patch-package": "^8.0.0",
    "zod": "^3.22.4"
  }
}
