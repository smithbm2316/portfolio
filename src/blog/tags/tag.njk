---
pagination:
  data: collections.postTags
  size: 1
  alias: tag
permalink: /blog/tags/{{ tag }}/
---

<h1>Tagged “{{ tag }}”</h1>
<ol>
  {% for post in collections[tag] | reverse %}
    {% if post.data.draft != true %}
      <li class='list-disc list-inside'>
        <a href="{{ post.url }}">{{ post.data.title }}</a>
      </li>
    {% endif %}
  {% endfor %}
</ol>
